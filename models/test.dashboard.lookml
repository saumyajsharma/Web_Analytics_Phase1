  - title: User Breakdown
    name: User Breakdown
    model: Web_Analytics_Phase1
    explore: sessions
    type: looker_donut
    fields: [unique_users, new_users]
    limit: 500
    column_limit: 50
    dynamic_fields:
    - category: measure
      expression: "${sessions.new_user_flag} = yes"
      label: New Users
      value_format:
      value_format_name:
      based_on: sessions.user_id
      filter_expression: "${sessions.new_user_flag} = yes"
      _kind_hint: measure
      measure: new_users
      type: count_distinct
      _type_hint: number
    - category: measure
      expression:
      label: Unique Users
      value_format:
      value_format_name:
      based_on: sessions.user_id
      _kind_hint: measure
      measure: unique_users
      type: count_distinct
      _type_hint: number
    - category: table_calculation
      expression: "${unique_users}-${new_users}"
      label: Returning Users
      value_format:
      value_format_name:
      _kind_hint: measure
      table_calculation: returning_users
      _type_hint: number
    hidden_fields: [unique_users]
    hidden_points_if_no: []
    series_labels:
      new_users: New Users
      returning_users: Returning Users
